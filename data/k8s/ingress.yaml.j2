apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: eightbitsaxlounge-data-ingress
  labels:
    app: eightbitsaxlounge
    component: data
spec:
  ingressClassName: nginx
  rules:
  - host: {{ data_ingress_host }}
    http:
      paths:
      - path: /data
        pathType: Prefix
        backend:
          service:
            name: eightbitsaxlounge-data-service
            port:
              number: 80
